{
  "manifest_version": 3,
  "name": "Capturador de URLs",
  "version": "1.0",
  "description": "Una extensi√≥n para capturar y descargar URLs de archivos .ts",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webRequest",
    "downloads",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}